{
  "rules": {
    "locks": {
      "$canvasId": {
        ".read": "auth != null",
        "$shapeId": {
          ".write": "auth != null"
        }
      }
    },
    "sessions": {
      "$canvasId": {
        ".read": "auth != null",
        "$userId": {
          ".write": "auth != null && auth.uid == $userId"
        }
      }
    },
    "drag": {
      "$canvasId": {
        ".read": "auth != null",
        "$shapeId": {
          ".write": "auth != null"
        }
      }
    }
  }
}

